              <conditions>
                <RotStage>Fresh,Rotting</RotStage>
                <BodyPart>
                  <bodyPartLabel>left ear</bodyPartLabel>
                  <drawWithoutPart>false</drawWithoutPart>
                </BodyPart>
                <Apparel>
