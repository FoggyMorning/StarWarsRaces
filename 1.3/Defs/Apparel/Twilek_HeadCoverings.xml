<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<!-- war veil stats -->
	<ThingDef ParentName="HatMakeableBase">
		<defName>StarWarsRaces_TwilekHeadCovering</defName>
		<label>twi'lek head wrap</label>
		<thingCategories>
			<li>StarWarsRaces_TwilekHatCategory</li>
		</thingCategories>
		<techLevel>Neolithic</techLevel>
		<recipeMaker>
			<researchPrerequisite>StarWarsRaces_TwilekBasicHats</researchPrerequisite>
			<recipeUsers>
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
				<li>CraftingSpot</li>
			</recipeUsers>
			<skillRequirements>
				<Crafting>1</Crafting>
			</skillRequirements>
		</recipeMaker>
		<description>A simple head covering with openings for Lekku.</description>
		<graphicData>
			<texPath>StarWarsRaces/Twilek/Hats/Head_Cover1_south</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<costStuffCount>20</costStuffCount>
		<stuffCategories>
			<li>Leathery</li>
			<li>Fabric</li>
		</stuffCategories>
		<statBases>
			<WorkToMake>1400</WorkToMake>
			<MaxHitPoints>100</MaxHitPoints>
			<Mass>0.08</Mass>
			<StuffEffectMultiplierArmor>0.2</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0.05</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.05</StuffEffectMultiplierInsulation_Heat>
			<EquipDelay>1.2</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<PainShockThreshold>0.05</PainShockThreshold>
		</equippedStatOffsets>
		<generateCommonality>0.01</generateCommonality>
		<apparel>
			<bodyPartGroups>
				<li>UpperHead</li>
			</bodyPartGroups>
			<wornGraphicPath>StarWarsRaces/Twilek/Hats/Head_Cover1</wornGraphicPath>
			<layers>
				<li>Overhead</li>
			</layers>
			<tags>
				<li>StarWarsRaces_TwilekApparelTag</li>
			</tags>
			<defaultOutfitTags>
				<li>StarWarsRaces_TwilekApparelTag</li>
			</defaultOutfitTags>
		</apparel>
		<colorGenerator Class="ColorGenerator_StandardApparel" />
	</ThingDef>


	<!-- a tuque -->
