<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <AlienRace.ThingDef_AlienRace ParentName="StarWarsRace_Base">
    <defName>StarWarsRaces_Ewok</defName>
    <label>Ewok</label>
    <description>The Ewoks are sentient furred bipeds native to the moon of Endor.</description>
    <alienRace>
      <thoughtSettings>
        <cannotReceiveThoughts>
          <li>ClothedNudist</li>
          <li>Naked</li>
        </cannotReceiveThoughts>

        <ateThoughtSpecific>
        </ateThoughtSpecific>

        <replacerList>
        </replacerList>
      </thoughtSettings>
      <generalSettings>
        <forcedRaceTraitEntries>
          <li>
            <defName>Nudist</defName>
            <chance>0</chance>
          </li>
        </forcedRaceTraitEntries>
        <alienPartGenerator>
          <customDrawSize>( 0.65, 0.65 )</customDrawSize>
          <customHeadDrawSize>( 0.65, 0.65 )</customHeadDrawSize>
          <customPortraitDrawSize>( 0.65, 0.65 )</customPortraitDrawSize>
          <customPortraitHeadDrawSize>( 0.65, 0.65 )</customPortraitHeadDrawSize>
          <headOffset>(0,-0.1)</headOffset>
          <useGenderedHeads>false</useGenderedHeads>
          <colorChannels>
            <li>
              <name>skin</name>
              <first Class="ColorGenerator_Options">
                <options>
                  <li>
                    <weight>10</weight>
                    <only>RGBA(0.623, 0.380, 0.149,1)</only>
                  </li>
                  <li>
                    <!-- #843e30 -->
                    <weight>10</weight>
                    <only>RGBA(0.517, 0.243, 0.188,1)</only>
                  </li>
                  <li>
                    <!-- #8b352d -->
                    <weight>10</weight>
                    <only>RGBA(0.545, 0.207, 0.176,1)</only>
                  </li>
                  <li>
                    <!-- #784b44 -->
                    <weight>10</weight>
                    <only>RGBA(0.470, 0.294, 0.266,1)</only>
                  </li>
                  <li>
                    <!-- #685855 -->
                    <weight>10</weight>
                    <only>RGBA(0.407, 0.345, 0.333,1)</only>
                  </li>
                  <li>
                    <!-- #764c41 -->
                    <weight>10</weight>
                    <only>RGBA(0.462, 0.298, 0.254,1)</only>
                  </li>
                </options>
              </first>
              <!-- eyes -->
              <second Class="ColorGenerator_Options">
                <options>
                  <li>
                    <weight>10</weight>
                    <only>RGBA(0.698, 0.615, 0.541,1)</only>
                  </li>
                  <li>
                    <weight>10</weight>
                    <only>RGBA(0.694, 0.478, 0.282,1)</only>
                  </li>
                </options>
              </second>
            </li>
            <li>
              <name>hair</name>
              <first Class="AlienRace.ColorGenerator_CustomAlienChannel">
                <colorChannel>skin_1</colorChannel>
              </first>
              <second Class="AlienRace.ColorGenerator_CustomAlienChannel">
                <colorChannel>skin_2</colorChannel>
              </second>
            </li>
          </colorChannels>
          <aliencrowntypes>
            <li>Female_Average_PatternA_Normal</li>
            <li>Female_Average_PatternB_Normal</li>
            <li>Male_Average_PatternA_Normal</li>
          </aliencrowntypes>
          <alienbodytypes>

            <!-- hidden
            <li>Oval</li>
            -->
          </alienbodytypes>
          <bodyAddons>
            <li>
              <drawnDesiccated>false</drawnDesiccated>
              <path>StarWarsRaces/Ewok/Ear/LeftEar</path>
              <bodyPart>Ear</bodyPart>
              <drawSize>0.65</drawSize>
              <inFrontOfBody>true</inFrontOfBody>
              <offsets>
                <south>
                  <layerOffset>0</layerOffset>
                  <offset>( -0.0, 0.22 )</offset>
                </south>
                <north>
                  <layerOffset>0</layerOffset>
                  <offset>( -0.0, 0.22 )</offset>
                </north>
                <east>
                  <layerOffset>0</layerOffset>
                  <offset>( -0.0, 0.0 )</offset>
                </east>
                <west>
                  <!-- hidden -->
                  <layerOffset>0</layerOffset>
                  <offset>( -0.1, 0.22 )</offset>
                </west>
              </offsets>
              <hiddenUnderApparelFor>
                <li>UpperHead</li>
              </hiddenUnderApparelFor>
              <hiddenUnderApparelTag >
                <li>StarWarsRaces_EwokEars</li>
              </hiddenUnderApparelTag>
            </li>
            <li>
              <drawnDesiccated>false</drawnDesiccated>
              <path>StarWarsRaces/Ewok/Ear/RightEar</path>
              <bodyPart>Ear</bodyPart>
              <drawSize>0.65</drawSize>
              <inFrontOfBody>true</inFrontOfBody>
              <offsets>
                <south>
                  <layerOffset>0</layerOffset>
                  <offset>( -0.0, 0.22 )</offset>
                </south>
                <north>
                  <layerOffset>0</layerOffset>
                  <offset>( -0.0, 0.22 )</offset>
                </north>
                <east>
                  <layerOffset>0</layerOffset>
                  <offset>( -0.1, 0.22 )</offset>
                </east>
                <west>
                  <!-- hidden -->
                  <layerOffset>0</layerOffset>
                  <offset>( 0.0, 0.0 )</offset>
                </west>
              </offsets>
              <hiddenUnderApparelFor>
                <li>UpperHead</li>
              </hiddenUnderApparelFor>
              <hiddenUnderApparelTag >
                <li>StarWarsRaces_EwokEars</li>
              </hiddenUnderApparelTag>
            </li>
          </bodyAddons>
        </alienPartGenerator>
        <humanRecipeImport>true</humanRecipeImport>
      </generalSettings>
      <styleSettings>
        <li>
          <key>HairDef</key>
          <value>
            <hasStyle>false</hasStyle>
          </value>
        </li>
        <li>
          <key>BeardDef</key>
          <value>
            <hasStyle>false</hasStyle>
          </value>
        </li>
        <li>
          <key>TattooDef</key>
          <value>
            <hasStyle>false</hasStyle>
          </value>
        </li>
      </styleSettings>
      <graphicPaths>
        <li>
          <head>StarWarsRaces/Ewok/Head/</head>
        </li>
      </graphicPaths>
    </alienRace>
    <tools>
      <li>
        <label>left fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>8.2</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>14</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>right fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>8.2</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>14</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>teeth</label>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>8.2</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
        <chanceFactor>0.07</chanceFactor>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>5</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
    <race>
      <leatherDef>StarWarsRaces_Leather_Humanlike</leatherDef>
      <nameGenerator>StarWarsRaces_NamerPersonEwokM</nameGenerator>
      <nameGeneratorFemale>StarWarsRaces_NamerPersonEwokF</nameGeneratorFemale>
    </race>
  </AlienRace.ThingDef_AlienRace>
</Defs>